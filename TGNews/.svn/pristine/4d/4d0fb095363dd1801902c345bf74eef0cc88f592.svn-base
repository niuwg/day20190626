.cl { clear: both; overflow: hidden; }
.fl { float: left; }
.fr { float: right; }
a { text-decoration: none; }
body { font: 12px/1.5 arial, '\5FAE\8F6F\96C5\9ED1', "Hiragino Sans GB", "Hiragino Sans GB W3", \5b8b\4f53; background:url(../images/tgyg_bg.jpg) no-repeat #242424 top center;}

#header{ width:100%; height:34px; background:url(../images/slidebox_bg.png);}
.header_content{ width:1000px; height:34px; margin:0 auto;}
.header_content img{ margin:5px 5px 5px 0;}
.header_content a{ color:#ccc; line-height:34px;}
.header_content a:hover{ color:#ffff00;}
.right15{ margin-right:15px;}

#content{ width:1000px; overflow:hidden; margin:430px auto 0; }
#part1{ width:950px; height:160px; background:url(../images/tgyg_cbg.png) no-repeat; padding:72px 25px 48px;}
#part1 .info_img{ width:246px; height:160px; display:block; float:left;}
#part1 .info_c{ width:660px; height:140px; float:right; font-size:16px; color:#fff; line-height:28px; padding:10px 0;}

#part2{ width:950px; height:358px; background:url(../images/tgyg_cbg.png) no-repeat 0 -300px; padding:87px 25px 42px; margin-top:60px;}
.part2_l{ float:left; width:430px; height:358px;}
.list_title{ width:410px; height:32px; background:url(../images/tgyg_cbg.png) no-repeat 0 -808px #141414; padding:0 10px; line-height:32px; text-align:right; color:#fff; border-bottom:1px solid 3292929;}
.list_title span{ margin:0 5px; color:#FF0;}
.part2_l ul{ width:430px; height:295px; background:#141414; padding-bottom:30px; display:block; overflow-y:scroll; overflow-x: hidden;}

/* Let's get this party started */
::-webkit-scrollbar {
    width: 10px;
}
 
/* Track */
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    -webkit-border-radius: 10px;
    border-radius: 10px;
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: rgba(87,87,87,0.8); 
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5); 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: rgba(0,0,0,0.4); 
}

.part2_l ul li{ width:370px; height:26px; display:block; margin:0 10px; background:url(../images/tgyg_cbg.png) no-repeat 0 -860px; padding:0 20px; border-bottom:1px solid #0f0f0f; border-top:1px solid #1a1a1a;}
.part2_l ul li a{ line-height:26px; color:#fff;}
.part2_l ul li a:hover{ color:#FF0;}
.part2_l ul li .on{ color:#FF0;}

.part2_r{ float:right; overflow:hidden;}
embed{ border:none;}

#footer{ margin:60px 0 20px;}
#footer p{ text-align:center; color:#ccc; line-height:24px; font-size:12px; font-size:12px;}
#footer p a{ color:#ccc;}
#footer p a:hover{ color:#fff;}
#footer p span{ margin:0 5px;}